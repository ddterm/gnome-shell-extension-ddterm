{
    "problemMatcher": [
        {
            "owner": "generic",
            "pattern": [
                {
                    "regexp": "^(?:\\.\\./)?([^\\s:]+):(\\d+)(?::(\\d+))?:?\\s+(?:((?i)warning|error):?)?\\s*(.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            ]
        },
        {
            "owner": "generic-nolocation",
            "pattern": [
                {
                    "regexp": "^(?:\\.\\./)?(?:([^\\s:]+):?\\s+)?((?i)warning|error):?\\s*(.+)$",
                    "file": 1,
                    "severity": 2,
                    "message": 3
                }
            ]
        }
    ]
}
