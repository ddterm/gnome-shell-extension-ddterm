{
    "problemMatcher": [
        {
            "owner": "gettext-stats",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "(?:^|\\s)(?:\\.\\./)?(locale/.*\\.po)$",
                    "file": 1
                },
                {
                    "regexp": "^((\\d+) translated messages?,.*)$",
                    "message": 1
                }
            ]
        }
    ]
}
